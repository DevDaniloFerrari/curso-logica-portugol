algoritmo "Tabuada"
// Função :
// Autor :
// Data : 10/11/2017
// Seção de Declarações 
var
 Cont, N, R : Inteiro
inicio
 Cont <- 1
      Escreva ("Qual a tabuada que você quer? ")
      Leia (N)
           Repita
                 R <- Cont * N
                 Escreval (N, " x ", cont, " = ",R)
                 Cont <- Cont + 1
           Ate (Cont > 10)
fimalgoritmo
